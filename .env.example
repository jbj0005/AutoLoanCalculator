# Copy to .env (local only). Vite will expose VITE_* at build time.

# Supabase (required for DB features)
VITE_SUPABASE_URL=
VITE_SUPABASE_ANON_KEY=

# Google (optional; leave blank to disable)
VITE_GMAPS_API_KEY=

# Feature toggles
VITE_ENABLE_GOOGLE_PLACES=false
VITE_ENABLE_GOOGLE_GEOCODING=true
VITE_GEOCODE_ON_INPUT=true
